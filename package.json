{
  "name": "eastward.js",
  "version": "0.0.45",
  "description": "Typescript library for imort/export assets of the video game Eastward.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "eastward": "dist/bin.js"
  },
  "scripts": {
    "test": "tsx watch ./test/index.ts",
