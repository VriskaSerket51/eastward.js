    "build": "rimraf ./dist && tsc && tsc-alias",
    "start": "npm run build && node ./dist/index.js",
    "dev": "tsx watch ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VriskaSerket51/eastward.js.git"
  },
