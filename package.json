  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/VriskaSerket51/eastward.js/issues"
  },
  "homepage": "https://github.com/VriskaSerket51/eastward.js#readme",
  "devDependencies": {
    "@types/lz4js": "^0.2.1",
    "@types/node": "^22.5.5",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@lunapaint/png-codec": "^0.2.0",
    "@metastable/cppzst": "^2.1.0",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "arg": "^5.0.2",
